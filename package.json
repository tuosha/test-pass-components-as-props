{
    "name": "webpack-product-project",
    "version": "1.0.0",
    "description": "webpack typescript",
    "main": "index.tsx",
    "scripts": {
        "build:prod": "cross-env NODE_ENV=production webpack --mode production",
        "build:dev": "cross-env NODE_ENV=development webpack --mode development",
        "start": "cross-env NODE_ENV=development webpack serve --open",
        "lint": "cross-env NODE_ENV=eslint_mode webpack serve --mode development --open",
        "lint:cli": "eslint --ignore-path .gitignore .",
        "lint:fix": "eslint --ignore-path .gitignore . --fix",
        "format": "prettier --check --ignore-path .gitignore .",
        "format:fix": "prettier --write --ignore-path .gitignore .",
        "stats": "cross-env NODE_ENV=bundle_analyzer webpack --mode production --json > stats.json && webpack-bundle-analyzer stats.json",
        "clean": "rd /s /q dist"
    },
    "license": "ISC",
    "author": "Anton",
    "devDependencies": {
        "@types/node": "^22.5.4",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/webpack": "^5.28.5",
        "@types/webpack-bundle-analyzer": "^4.7.0",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "csv-loader": "^3.0.5",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.35.2",
        "eslint-webpack-plugin": "^4.2.0",
        "html-webpack-plugin": "^5.6.0",
        "less": "^4.2.0",
        "less-loader": "^12.2.0",
        "mini-css-extract-plugin": "^2.9.1",
        "sass": "^1.79.1",
        "sass-loader": "^16.0.1",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "webpack": "^5.94.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.27.0"
    }
}
